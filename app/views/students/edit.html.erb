<% if flash[:error] %>
<% flash[:error].each do |msg| %>
<li><%= msg %></li>
<% end %>
<% end %>


<h1>Editing <%= @student.name%></h1>
<%= form_for @student do |f| %>
<div>
<%= f.label :name %>
<%= f.text_field :name%>
</div>
<div>
<%= f.label :mod %>
<%= f.text_field :mod%>
</div>
<%= f.submit%>
<% end %>